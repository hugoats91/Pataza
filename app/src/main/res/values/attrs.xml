<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="SpinnerView">
        <attr name="hint" format="string" />
        <attr name="type">
            <flag name="single" value="1" />
            <flag name="multiple" value="2" />
            <flag name="color" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="EditTextView">
        <attr name="hint"/>
        <attr name="action" format="boolean" />
        <attr name="input_type">
            <flag name="text" value="1" />
            <flag name="number" value="2" />
            <flag name="password" value="3" />
        </attr>
    </declare-styleable>
</resources>